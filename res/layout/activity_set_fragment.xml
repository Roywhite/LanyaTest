<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
	<include android:id="@+id/head_name_in_set" layout="@layout/head"/>
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:scrollbars="vertical"
        android:fadingEdge="vertical"
        android:layout_below="@id/head_name_in_set">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:id="@+id/set_dark"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="17dp"
            android:background="#E8E8E8"
            android:text="夜间模式"
            android:textSize="19dp"
            />
        <TextView
            android:id="@+id/set_dark1"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="17dp"
            android:background="#E8E8E8"
            android:text="夜间模式"
            android:textSize="19dp" />
        <TextView
            android:id="@+id/set_dark2"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="17dp"
            android:background="#E8E8E8"
            android:text="夜间模式"
            android:textSize="19dp" />
        <TextView
            android:id="@+id/set_dark3"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="17dp"
            android:background="#E8E8E8"
            android:text="夜间模式"
            android:textSize="19dp" />
        <TextView
            android:id="@+id/set_dark4"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="17dp"
            android:background="#E8E8E8"
            android:text="夜间模式"
            android:textSize="19dp" />
        </LinearLayout>
    </ScrollView>
</RelativeLayout>
